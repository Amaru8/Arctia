{
  "name": "cider-remote",
	"displayName": "Cider Remote",
	"description": "This extension is a remote for the open-source Apple Music client, Cider",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:cider-remote.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
            "command": "cider-remote.helloWorld",
            "title": "Hello World"
		},
		{
			"command": "cider-remote.startPlayback",
			"title": "Start Playback"
		},
		{
			"command": "cider-remote.stopPlayback",
			"title": "Stop Playback"
		},
		{
			"command": "cider-remote.playNext",
			"title": "Play Next"
		},
		{
			"command": "cider-remote.playPrevious",
			"title": "Play Previous"
		},
		{
			"command": "cider-remote.toggleShuffle",
			"title": "Toggle Shuffle"
		},
		{
			"command": "cider-remote.toggleRepeat",
			"title": "Toggle Repeat"
		},
		{
			"command": "cider-remote.toggleMute",
			"title": "Toggle Mute"
		},
		{
			"command": "cider-remote.setVolume",
			"title": "Set Volume"
		},
		{
			"command": "cider-remote.setPlaybackPosition",
			"title": "Set Playback Position"
		},
		{
			"command": "cider-remote.setPlaybackSpeed",
			"title": "Set Playback Speed"
		},
		{
			"command": "cider-remote.setPlaybackShuffle",
			"title": "Set Playback Shuffle"
		},
		{
			"command": "cider-remote.setPlaybackRepeat",
			"title": "Set Playback Repeat"
		}]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.66.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "14.x",
        "eslint": "^8.11.0",
        "glob": "^7.2.0",
        "mocha": "^9.2.2",
        "typescript": "^4.5.5",
        "@vscode/test-electron": "^2.1.3"
    }

}
